            steps{
                checkout scmGit(branches: [[name: 'test']], extensions: [], userRemoteConfigs: [[credentialsId: 'GitHub-Jenkins_key', url: 'https://github.com/Hrishabh1811/two_tier_app.git']]) 
            }
        }
        stage("Build & Test"){
            steps{
                echo "Hello form branch sudo-main"
                
            }
        }
    }
}
